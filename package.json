{
  "name": "nevio-pwa",
  "version": "1.3.9",
  "description": "A PWA for Nevio Audioguide",
  "keywords": [],
  "main": "index.js",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "start": "node ./node_modules/gulp/bin/gulp.js dev",
